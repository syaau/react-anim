{
  "name": "react-animation",
  "version": "1.0.0",
  "description": "React Animation Library for the Web",
  "files": [
    "*.md",
    "lib",
    "es6",
    "umd"
  ],
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node demo/server.js",
    "build": "npm run build-cjs && npm run build-es6",
    "build-cjs": "rimraf lib && babel ./src -d lib --presets es2015,stage-0,react --ignore '__tests__'",
    "build-es6": "rimraf es6 && babel ./src -d es6 --presets es2015,stage-0,react --blacklist=es6.modules --ignore '__tests__'",
    "build-umd": "NODE_ENV=production webpack src/index.js umd/ReactAnimation.js",
    "build-min": "NODE_ENV=production webpack -p src/index.js umd/ReactAnimation.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syaau/react-animation.git"
  },
  "author": "Ranjan Shrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syaau/react-animation/issues"
  },
  "homepage": "https://github.com/syaau/react-animation#readme",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "brfs": "^1.4.3",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "express-urlrewrite": "^1.2.0",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "path": "^0.12.7",
    "react-canvas": "^1.1.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "transform-loader": "^0.2.3",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
